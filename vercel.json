{
    "builds": [
      {
        "src": "genQuo/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLamdaSize":"15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
      {
        "src": "/(.*)",
        "dest": "genQuo/wsgi.py"
      }
    ]
}